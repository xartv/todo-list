{
  "name": "todo-list",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "VITE_PROJECT=frontend vite",
    "start:dev": "concurrently \"yarn start\" \" yarn start:dev:server\"",
    "start:dev:server": "node ./json-server/index.cjs",
    "build": "VITE_BASE_URL=https://production-todolist-server.vercel.app tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "lint:styles": "stylelint src/**/*.scss",
    "lint:styles:fix": "stylelint src/**/*.scss --fix",
    "format": "prettier --write src",
    "format:check": "prettier --check .",
    "preview": "vite preview",
    "test:unit": "VITE_PROJECT=vitest vitest",
    "test:ui": "yarn loki test",
    "test:ui:ci": "loki --requireReference --reactUri file:./storybook-static",
    "test:ui:ok": "yarn loki approve",
    "test:ui:json": "node scripts/generate-visual-json-report.cjs",
    "test:ui:html": "yarn reg-cli --from .loki/report.json --report .loki/report.html",
    "test:ui:report": "yarn test:ui:json && yarn test:ui:html",
    "sb": "VITE_PROJECT=storybook storybook dev -p 6006",
    "sb:build": "VITE_PROJECT=storybook storybook build",
    "server": "json-server --watch json-server/db.json",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@reduxjs/toolkit": "^1.9.5",
    "axios": "^1.4.0",
    "classnames": "^2.3.2",
    "dotenv": "^16.3.1",
    "eslint-plugin-react": "^7.32.2",
    "framer-motion": "^10.16.4",
    "i18next": "^22.5.1",
    "i18next-browser-languagedetector": "^7.0.2",
    "i18next-http-backend": "^2.2.1",
    "lodash": "^4.17.21",
    "postcss": "^8.4.24",
    "postcss-load-config": "^4.0.1",
    "postcss-scss": "^4.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.3.1",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.11.2",
    "react-select": "^5.7.3",
    "react-use": "^17.4.0",
    "sass": "^1.62.1",
    "vite-plugin-svgr": "^3.2.0",
    "vite-tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.0.2",
    "@storybook/addon-essentials": "^7.0.20",
    "@storybook/addon-interactions": "^7.0.20",
    "@storybook/addon-links": "^7.0.20",
    "@storybook/blocks": "^7.0.20",
    "@storybook/react": "^7.0.20",
    "@storybook/react-vite": "^7.0.20",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.14.194",
    "@types/node": "^20.2.5",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "@vitest/ui": "^0.34.5",
    "concurrently": "^8.2.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-i18next": "^6.0.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "eslint-plugin-storybook": "^0.6.12",
    "eslint-plugin-ulbi-tv-plugin": "^0.0.10",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jsdom": "^22.1.0",
    "json-server": "^0.17.3",
    "loki": "^0.32.0",
    "prettier": "^2.8.8",
    "prop-types": "^15.8.1",
    "reg-cli": "^0.17.7",
    "rollup-plugin-visualizer": "^5.9.2",
    "storybook": "^7.0.20",
    "storybook-react-i18next": "^2.0.6",
    "stylelint": "^15.7.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recommended": "^12.0.0",
    "stylelint-order": "^6.0.3",
    "stylelint-prettier": "^3.0.0",
    "stylelint-scss": "^5.0.1",
    "typescript": "^5.0.2",
    "vite": "^4.3.2",
    "vite-aliases": "^0.11.2",
    "vite-jest": "^0.1.4",
    "vite-plugin-alias": "^0.0.8",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-stylelint": "^4.3.0",
    "vitest": "^0.32.0"
  },
  "loki": {
    "configurations": {
      "chrome.laptop": {
        "target": "chrome.docker",
        "width": 1366,
        "height": 768,
        "deviceScaleFactor": 1,
        "mobile": false
      },
      "chrome.iphone7": {
        "target": "chrome.docker",
        "preset": "iPhone 7"
      }
    }
  }
}
